        {
          "host": "customers.internal.prod",
          "port": 443
        }
      ],
      "tags": [
        "customers",
        "core"
      ],
      "healthEndpoint": "/health"
    },
    {
      "id": "svc-inventory",
      "environmentId": "env-prod",
      "name": "Inventory Service",
