  "routes": [
    {
      "id": "route-orders-v1",
      "environmentId": "env-prod",
      "routeKey": "orders-v1",
      "description": "Orders API v1 - Main orders endpoint",
      "upstreamPathTemplate": "/api/v1/orders/{*everything}",
      "upstreamMethods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "downstreamPathTemplate": "/orders/{everything}",
      "downstreamServiceId": "svc-orders",
      "priority": 1,
      "requestIdKey": "X-Correlation-ID",
      "policies": {
        "auth": {
          "scheme": "oidc",
          "allowedScopes": [
            "orders.read",
            "orders.write"
          ]
        },
        "rateLimit": {
          "enableRateLimiting": true,
          "period": "1m",
          "limit": 100
        },
        "cache": {
          "ttlSeconds": 60
        }
      }
    },
    {
      "id": "route-payments-v1",
      "environmentId": "env-prod",
      "routeKey": "payments-v1",
      "description": "Payments API v1 - Payment processing",
      "upstreamPathTemplate": "/api/v1/payments/{*everything}",
      "upstreamMethods": [
        "GET",
        "POST"
      ],
      "downstreamPathTemplate": "/payments/{everything}",
      "downstreamServiceId": "svc-payments",
      "priority": 1,
      "requestIdKey": "X-Correlation-ID",
      "policies": {
        "auth": {
          "scheme": "oidc",
          "allowedScopes": [
            "payments.read",
            "payments.write"
          ]
        },
        "rateLimit": {
          "enableRateLimiting": true,
          "period": "1m",
          "limit": 50
        },
        "cache": {
          "ttlSeconds": 0
        }
      }
    },
    {
      "id": "route-customers-v1",
      "environmentId": "env-prod",
      "routeKey": "customers-v1",
      "description": "Customers API v1 - Customer management",
      "upstreamPathTemplate": "/api/v1/customers/{*everything}",
      "upstreamMethods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE"
      ],
      "downstreamPathTemplate": "/customers/{everything}",
      "downstreamServiceId": "svc-customers",
      "priority": 1,
      "requestIdKey": "X-Correlation-ID",
      "policies": {
        "auth": {
          "scheme": "oidc",
          "allowedScopes": [
            "customers.read",
            "customers.write"
          ]
        },
        "rateLimit": {
          "enableRateLimiting": true,
          "period": "1m",
          "limit": 200
        },
        "cache": {
          "ttlSeconds": 120
        }
      }
    },
    {
      "id": "route-customers-v2",
      "environmentId": "env-uat",
      "routeKey": "customers-v2",
      "description": "Customers API v2 - New customer endpoints with enhanced features",
      "upstreamPathTemplate": "/api/v2/customers/{*everything}",
      "upstreamMethods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE"
      ],
      "downstreamPathTemplate": "/v2/customers/{everything}",
      "downstreamServiceId": "svc-customers",
      "priority": 1,
      "requestIdKey": "X-Correlation-ID",
      "policies": {
        "auth": {
          "scheme": "oidc",
          "allowedScopes": [
            "customers.read",
            "customers.write"
          ]
        },
        "rateLimit": {
          "enableRateLimiting": true,
          "period": "1m",
          "limit": 200
        },
        "cache": {
          "ttlSeconds": 120
        }
      }
    },
    {
      "id": "route-inventory-v1",
      "environmentId": "env-prod",
      "routeKey": "inventory-v1",
      "description": "Inventory API v1 - Stock management",
      "upstreamPathTemplate": "/api/v1/inventory/{*everything}",
      "upstreamMethods": [
        "GET",
        "POST",
        "PUT"
      ],
      "downstreamPathTemplate": "/inventory/{everything}",
      "downstreamServiceId": "svc-inventory",
      "priority": 1,
      "requestIdKey": "X-Correlation-ID",
      "policies": {
        "auth": {
          "scheme": "oidc",
          "allowedScopes": [
            "inventory.read",
            "inventory.write"
          ]
        },
        "rateLimit": {
          "enableRateLimiting": true,
          "period": "1m",
          "limit": 150
        },
        "cache": {
          "ttlSeconds": 30
        }
      }
    }
  ],
