      "hosts": [
        {
          "host": "payments.internal.prod",
          "port": 443
        }
      ],
      "tags": [
        "payments",
        "core",
        "pci"
      ],
      "healthEndpoint": "/health"
    },
    {
      "id": "svc-customers",
      "environmentId": "env-prod",
      "name": "Customers Service",
